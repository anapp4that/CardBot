{
  "name": "CardBot",
  "description": "A Slack bot and Web app used for creating match cards for events like wrestling or MMA",
  "repository": "https://github.com/anapp4that/CardBot",
  "keywords": ["bots", "card", "chatbots", "match", "slack", "web"],
  "website": "https://github.com/anapp4that/CardBot",
  "success_url": "/",
  "addons": [
    {
      "plan": "mongolab",
      "as": "MONGO"
    }
  ],
  "env": {
    "clientId": {
      "description": "Client ID provided by Slack"
    },
    "clientSecret": {
      "description": "Client Secret provided by Slack"
    },
    "studio_token": {
      "description": "Token associated with your bot in Botkit Studio",
      "required": false
    }
  }
}
